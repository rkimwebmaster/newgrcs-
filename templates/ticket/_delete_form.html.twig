<form method="post" action="{{ path('app_ticket_delete', {'id': ticket.id}) }}" onsubmit="return confirm('Voulez-vous vraiment supprimer définitivement cet élément?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ ticket.id) }}">
    <button class="btn btn-default "> <span class="fa fa-times-circle " > Supprimer  </button>
</form>
